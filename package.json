{
  "name": "googlemaps-tracking-view-demo",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --vendorSourceMap",
    "build": "ng build ngx-googlemaps-tracking-view",
    "build:demo": "cd demo && ng build --base-href=\".\"",
    "compodoc": "npx compodoc -p ./projects/ngx-googlemaps-tracking-view/tsconfig.lib.json -d dist/demo/reference",
    "predeploy": "rm -f -r dist && ng build ngx-googlemaps-tracking-view && npm run link:library",
    "deploy": "npm run build:demo && npm run compodoc && npx ngh --dir=\"demo/www\"",
    "prepublish": "rm -f -r dist",
    "publish": "npm run build && cp README.md dist/ngx-googlemaps-tracking-view && npm publish ./dist/ngx-googlemaps-tracking-view",
    "install:library": "cd projects/ngx-googlemaps-tracking-view && npm install",
    "link:library": "cd dist/ngx-googlemaps-tracking-view && npm link && cd ../.. && npm link ngx-googlemaps-tracking-view",
    "postinstall": "npm run install:library && npm run build && npm run link:library",
    "watch": "ng build ngx-googlemaps-tracking-view --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "core-js": "^3.6.4",
    "rxjs": "~6.5.5",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/googlemaps": "^3.37.3",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "~5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^9.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.8.2",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~6.1.1",
    "typescript": "~3.8.3"
  }
}
